public class assignmenet_operators {
    public static void main(String args[]){
        int A=10;
        int B=10;
        int C=10;
        int D=10;
        //  A=10+5;
            A+=5;//fater than previous statement
            B-=5;
            C*=5;
            D/=5;
        System.out.println(A);
        System.out.println(B);
        System.out.println(C);
        System.out.println(D);
    }
}
/*
output:-
15  // A=A+5
5  // B=B-5
50 // C=C*5
2  // D=D/5
*/
