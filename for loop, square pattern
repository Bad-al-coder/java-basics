public class for_loop {
    public static void main(String args[]){

        for(int i=1; i<=4; i++){
            System.out.println("bharat mata ki jai");
        }
    
//print square pattern with "*" :-

    for(int line=1; line<=4; line++){
        System.out.println("*  *  *  *");
    }
}
}


/*
output:-
bharat mata ki jai
*  *  *  *
*  *  *  *
*  *  *  *
*  *  *  *
*/
